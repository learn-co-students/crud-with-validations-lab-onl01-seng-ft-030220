<h1>All Songs</h1>

<%= link_to('Make a Song', new_song_path) %>

<table>
  <tr>
    <th>Song title</th>
    <th>Release Year</th>
    <th>Released</th>
    <th>Genre</th>
    <th>Artist Name</th>
  </tr>
  <tbody>
  <% @songs.each do |song| %>
  <tr>
    <td><%= link_to(song.title, song_path(song)) %></td>
    <td><%= song.release_year %></td>
    <td><%= song.released %></td>
    <td><%= song.genre %></td>
    <td><%= song.artist_name %></td>
  </tr>
  <% end %>
  </tbody>
</table>